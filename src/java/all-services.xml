<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
		default-init-method="init">

	<bean name="BaseService" abstract="true" class="xc.mst.manager.BaseService" >
		<property name="emailConfigDAO" ref="EmailConfigDAO" />
		<property name="harvestDAO" ref="HarvestDAO" />
		<property name="harvestRecordUtilDAO" ref="HarvestRecordUtilDAO" />
		<property name="harvestScheduleDAO" ref="HarvestScheduleDAO" />
		<property name="harvestScheduleStepDAO" ref="HarvestScheduleStepDAO" />
		<property name="logDAO" ref="LogDAO" />
		<property name="jobDAO" ref="JobDAO" />
		<property name="processingDirectiveDAO" ref="ProcessingDirectiveDAO" />
		<property name="processingDirectiveInputFormatUtilDAO" ref="ProcessingDirectiveInputFormatUtilDAO" />
		<property name="processingDirectiveInputSetUtilDAO" ref="ProcessingDirectiveInputSetUtilDAO" />
		<property name="formatDAO" ref="FormatDAO" />
		<property name="providerDAO" ref="ProviderDAO" />
		<property name="providerFormatUtilDAO" ref="ProviderFormatUtilDAO" />
		<property name="setDAO" ref="SetDAO" />
		<property name="recordTypeDAO" ref="RecordTypeDAO" />
		<property name="resumptionTokenDAO" ref="ResumptionTokenDAO" />
		<property name="xcIdentifierForFrbrElementDAO" ref="XcIdentifierForFrbrElementDAO" />
		<property name="errorCodeDAO" ref="ErrorCodeDAO" />
		<property name="oaiIdentifierForServiceDAO" ref="OaiIdentifierForServiceDAO" />
		<property name="serviceDAO" ref="ServiceDAO" />
		<property name="serviceInputFormatUtilDAO" ref="ServiceInputFormatUtilDAO" />
		<property name="serviceOutputFormatUtilDAO" ref="ServiceOutputFormatUtilDAO" />
		<property name="serviceOutputSetUtilDAO" ref="ServiceOutputSetUtilDAO" />
		<property name="groupDAO" ref="GroupDAO" />
		<property name="groupPermissionUtilDAO" ref="GroupPermissionUtilDAO" />
		<property name="permissionDAO" ref="PermissionDAO" />
		<property name="serverDAO" ref="ServerDAO" />
		<property name="userDAO" ref="UserDAO" />
		<property name="userGroupUtilDAO" ref="UserGroupUtilDAO" />
		<property name="dBRecordDAO" ref="DBRecordDAO" />
	</bean>

	<bean name="EmailConfigService" class="xc.mst.manager.configuration.DefaultEmailConfigService" parent="BaseService" />
	<bean name="ScheduleService" class="xc.mst.manager.harvest.DefaultScheduleService" parent="BaseService" />
	<bean name="LogService" class="xc.mst.manager.logs.DefaultLogService" parent="BaseService" />
	<bean name="JobService" class="xc.mst.manager.processingDirective.DefaultJobService" parent="BaseService" />
	<bean name="ProcessingDirectiveService" class="xc.mst.manager.processingDirective.DefaultProcessingDirectiveService" parent="BaseService" />
	<bean name="ServicesService" class="xc.mst.manager.processingDirective.DefaultServicesService" parent="BaseService" />
	<bean name="BrowseRecordService" class="xc.mst.manager.record.DefaultBrowseRecordService" parent="BaseService" />
	<bean name="ExpressionService" class="xc.mst.manager.record.DefaultExpressionService" parent="BaseService" />
	<bean name="HoldingsService" class="xc.mst.manager.record.DefaultHoldingsService" parent="BaseService" />
	<bean name="ItemService" class="xc.mst.manager.record.DefaultItemService" parent="BaseService" />
	<bean name="DefaultRecordService" class="xc.mst.manager.record.DefaultRecordService" parent="BaseService" />
	<bean name="DBRecordService" class="xc.mst.manager.record.DBRecordService" parent="BaseService" />
	<bean name="ManifestationService" class="xc.mst.manager.record.DefaultManifestationService" parent="BaseService" />
	<bean name="WorkService" class="xc.mst.manager.record.DefaultWorkService" parent="BaseService" />
	<bean name="FormatService" class="xc.mst.manager.repository.DefaultFormatService" parent="BaseService" />
	<bean name="ProviderService" class="xc.mst.manager.repository.DefaultProviderService" parent="BaseService" />
	<bean name="SetService" class="xc.mst.manager.repository.DefaultSetService" parent="BaseService" />
	<bean name="GroupService" class="xc.mst.manager.user.DefaultGroupService" parent="BaseService" />
	<bean name="PermissionService" class="xc.mst.manager.user.DefaultPermissionService" parent="BaseService" />
	<bean name="ServerService" class="xc.mst.manager.user.DefaultServerService" parent="BaseService" />
	<bean name="UserGroupUtilService" class="xc.mst.manager.user.DefaultUserGroupUtilService" parent="BaseService" />
	<bean name="UserService" class="xc.mst.manager.user.DefaultUserService" parent="BaseService" />
	
	<alias name="${record.service}" alias="RecordService" />
</beans>
